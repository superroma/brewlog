{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended", 
    "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },  
  "globals": {
    "document": true,
    "require": true,
    "module": true,
    "__dirname": true,
    "console": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}